      DTS:ObjectName="Create Temp Fact ManufSales Table"
      DTS:TaskContact="Execute SQL Task; Microsoft Corporation; SQL Server 2019; © 2019 Microsoft Corporation; All Rights Reserved;http://www.microsoft.com/sql/support/default.asp;1"
      DTS:ThreadHint="0">
      <DTS:Variables />
      <DTS:ObjectData>
        <SQLTask:SqlTaskData
          SQLTask:Connection="{C34F024B-1236-42D8-81FF-B4B63B6B576A}"
          SQLTask:SqlStatementSource="select   &#xA;  categorycode, &#xA;  weekno, &#xA;  manuf_code,&#xA;  manuf_upc,&#xA;  qty_prod_sold,&#xA;cnt_manuf, &#xA;  sum_delt_manuf &#xA;&#xA;  into temp_Fact_ManufacturerSales&#xA;&#xA;from &#xA;  (&#xA;    Select &#xA;      *, &#xA;      sum(cnt_manuf - old_cnt_manuf) over (partition by categorycode, weekno) as sum_delt_manuf &#xA;    from &#xA;      (&#xA;        select &#xA;          *, &#xA;          lag(cnt_manuf) over (&#xA;            partition by categorycode &#xA;            order by &#xA;              weekno&#xA;          ) as old_cnt_manuf, &#xA;          cnt_manuf -(&#xA;            lag(cnt_manuf) over (&#xA;              partition by categorycode &#xA;              order by &#xA;                weekno&#xA;            )&#xA;          ) as delt_manuf &#xA;        from &#xA;          (&#xA;            select &#xA;              categorycode, &#xA;              weekno, &#xA;              manuf_code,&#xA;&#x9;&#x9;&#x9;  manuf_upc,&#xA;              count(manuf_code) over (partition by categorycode, weekno) as cnt_manuf, &#xA;              sum(QtySld) as qty_prod_sold &#xA;            from &#xA;              (&#xA;                select &#xA;                  Weekno, &#xA;                  CategoryCode, &#xA;                  manuf_code,&#xA;&#x9;&#x9;&#x9;&#x9;  manuf_upc,&#xA;                  sum(QtySld) as QtySld&#xA;&#xA;                FROM &#xA;                  [TEMP_prd_cat] &#xA;                group by &#xA;                  Weekno, &#xA;                  CategoryCode, &#xA;                  manuf_code,&#xA;&#x9;&#x9;&#x9;&#x9;  manuf_upc&#xA;              ) a &#xA;            group by &#xA;              Weekno, &#xA;              CategoryCode, &#xA;              manuf_code,&#xA;&#x9;&#x9;&#x9;  manuf_upc&#xA;          ) a&#xA;      ) a&#xA;  ) a &#xA;;" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask" />
      </DTS:ObjectData>
    </DTS:Executable>
    <DTS:Executable
      DTS:refId="Package\Create Temp Prd-Cat Table"
      DTS:CreationName="Microsoft.ExecuteSQLTask"
      DTS:Description="Execute SQL Task"
      DTS:DTSID="{8447cbb7-1972-4fac-826b-e523968d6f92}"
      DTS:ExecutableType="Microsoft.ExecuteSQLTask"
      DTS:LocaleID="-1"
      DTS:ObjectName="Create Temp Prd-Cat Table"
      DTS:ThreadHint="0">
      <DTS:Variables />
      <DTS:ObjectData>
        <SQLTask:SqlTaskData
          SQLTask:Connection="{C34F024B-1236-42D8-81FF-B4B63B6B576A}"
          SQLTask:SqlStatementSource="select *, max(cat_upc) over (partition by CategoryCode, manuf_code order by CategoryCode)as manuf_upc into TEMP_prd_cat from&#xA;(&#xA;select&#xA;&#x9;&#x9;&#xA;&#x9;   cat.[CategoryCode]&#xA;      ,cat.[UPC] as cat_upc,&#xA;&#x9;  replace(cat.[UPC],substring(cat.[UPC], LEN(cat.[UPC])-5,5),'') as manuf_code &#xA;      ,cat.[CommodityCode]&#xA;      ,cat.[ProductName]&#xA;      ,cat.[ItemNumber]&#xA;&#x9;  ,prod.[UPC] as prod_upc&#xA;      ,prod.[Store]&#xA;      ,prod.[Weekno]&#xA;      ,prod.[QtySld]&#xA;      ,prod.[QtyBundle]&#xA;      ,prod.[Price]&#xA;      ,prod.[Profit]&#xA;&#x9;  ,REPLACE(PromotionType,'&quot;','') as PromotionType&#xA;&#x9;  ,CASE WHEN REPLACE(PromotionType,'&quot;','') = 'B' then 'Bonus Buy'&#xA;&#x9;&#x9;&#x9;WHEN REPLACE(PromotionType,'&quot;','') = 'C' then 'Coupon'&#xA;&#x9;&#x9;&#x9;WHEN REPLACE(PromotionType,'&quot;','') = 'S' then 'Sale' &#xA;&#x9;&#x9;&#x9;ELSE 'Not Available' &#xA;&#x9;   END as PromotionName &#xA;&#xA;from (select * from Product_Staging) prod&#xA;Right JOIN &#xA;(select * from Category_Staging) cat&#xA;ON cat.UPC = prod.upc &#xA;&#xA;) a WHERE prod_upc is not null&#xA;and cat_upc is not null&#xA;;" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask" />
      </DTS:ObjectData>
    </DTS:Executable>
